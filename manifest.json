{
  "name": "Yt tab playlist - id: yt-tab-playlist@oxie.cc",
  "short_name": "YTP",
  "description": "Use your tab bar as a playlist!",
  "manifest_version": 2,
  "version": "0.0.3",
  "author": "Mathieu CAROFF",
  "icons": {
    "32": "icon/iconB32.png",
    "48": "icon/iconB48.png",
    "64": "icon/iconB64.png",
    "128": "icon/iconB128.png",
    "256": "icon/iconB256.png"
  },

  "background": {
    "scripts": ["js/background.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["js/content.js"]
    }
  ],
  "permissions": ["tabs"]
}
